<div class="container-owner" style="position:relative;">
  <%= link_to 'Create a new tree', new_tree_path, class: "btn"%>

  <%=link_to 'See Your Rentals', owner_rentals_path, class: "btn" %>
</div>

<% @trees.each do |tree|%>
  <div class="card-owner-tree" style="position:relative;">
    <div class="text-card">
      <h1><%= tree.name %></h1>
      <h2 style=" text-align:center;"><%= tree.description %></h2>
      <h2 style="font-weight: bold; text-align: center; top: 9em; margin-top: 2em;"><%= tree.address %></h2>
      <ul>
        <li style="text-decoration: underline;"><h3><%= tree.species %></h3></li>
        <li><h3><%= tree.size.capitalize %></h3></li>
        <li><h3><%= tree.weight %></h3></li>
      </ul>
      <h1>$<%= tree.price %></h1>
    </div>
    <div class="card-photo" style="background-image: url(<%= cl_image_path tree.photo.key %>)"></div>
  </div>
<% end %>
</div>

